		<display-name>UpdateDo</display-name>
		<servlet-name>UpdateDo</servlet-name>
		<servlet-class>etp.note.servlet.UpdateDo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateDo</servlet-name>
		<url-pattern>/UpdateDo</url-pattern>
	</servlet-mapping>
