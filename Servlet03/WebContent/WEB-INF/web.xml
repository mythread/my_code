<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Servlet03</display-name>
  <listener>
    <listener-class>etp.listener.OnLineDemo</listener-class>
  </listener>
    <!--注释掉，避免与OnLineDemo产生冲突
  <listener>
    <listener-class>etp.listener.AppListener</listener-class>
  </listener>
  <listener>
    <listener-class>etp.listener.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>etp.listener.RequestListener</listener-class>
  </listener>
  -->
  <filter>
		<filter-name>SetCharacterEncodingFilter</filter-name>
		<filter-class>etp.filter.SetCharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>GB2312</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>SetCharacterEncodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
  <servlet>
    <description></description>
    <display-name>TestListener</display-name>
    <servlet-name>TestListener</servlet-name>
    <servlet-class>etp.test.TestListener</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestListener</servlet-name>
    <url-pattern>/TestListener</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>OnlineServlet</display-name>
    <servlet-name>OnlineServlet</servlet-name>
    <servlet-class>etp.online.OnlineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OnlineServlet</servlet-name>
    <url-pattern>/OnlineServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>etp.online.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
   <welcome-file-list>
    <welcome-file>online.html</welcome-file>
  </welcome-file-list>
</web-app>