<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Double"
		precision="22" not-null="false">
	</sql-type>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Integer"
		precision="2" not-null="true">
	</sql-type>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Integer"
		precision="4" not-null="true">
	</sql-type>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Integer"
		precision="4" not-null="false">
	</sql-type>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Double"
		precision="7" not-null="false">
	</sql-type>
	<sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Integer"
		precision="2" not-null="false">
	</sql-type>
</type-mapping>
<table-filter match-schema="SCOTT" match-name="BONUS"/>
  <table-filter match-schema="SCOTT" match-name="DEPT"/>
  <table-filter match-schema="SCOTT" match-name="EMP"/>
  <table-filter match-schema="SCOTT" match-name="SALGRADE"/>
</hibernate-reverse-engineering>